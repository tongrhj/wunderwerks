html{
	overflow-x:hidden;
}
body{
	/*font-family: 'PT Sans', sans-serif;*/
	font-family: 'Montserrat', sans-serif;
	font-weight:400;
	padding-bottom:0;
}
h1{
	font-size:72px;
	font-family: 'Cinzel', serif;
	font-weight:800;
	line-height:45px;
}
h2{
	font-size:54px;
	font-family: 'Cinzel', serif;
	font-weight:800;
	color:#eb590e;
	margin-bottom:20px;
}
h3{
	font-family: 'Cinzel', serif;
	font-weight:800;
	font-size:36px;
	margin-top:0;
}
h4{
	font-size:24px;
	font-family: 'Montserrat', sans-serif;
	font-weight:400;
	color:#866a5b;
	margin-bottom:0px;
}
h5{
	font-size:22px;
	font-family: 'Montserrat', sans-serif;
	font-weight:800;
	/*color:#8a8583;*/
	color:#866a5b;
	margin-bottom:0px;
	line-height:36px;
}
p, li{
	font-size:16px;
	font-family: 'Montserrat', sans-serif;
	font-weight:400;
}
a{
	font-family: 'Montserrat', sans-serif;
	font-weight:400;
	color:#eb590e;
}
.page{
	background:#f2e2d2;
}
.navbar{
	height:100px;
	padding-top:0px;
}
/*.navbar-inverse{*/
.navbar-inner{
	background-color:#f2e2d2;
    border-color:#aaa;
	border:none;
	border-top:1px solid #aaa;
}
.navbar-inverse{
	background-color:#f2e2d2;
    border-color:#aaa;
	border:none;
}
.navbar-inverse .navbar-nav>li>a{
	font-size:20px;
	color:#000;
	padding-top:0;
}
.navbar-left{
	margin-left:0px;
}
.navbar-right{
	margin-right:25px;
}
.navbar-inverse .navbar-nav.navbar-left>li:first-child{	
	width:255px;
}
.navbar-inverse .navbar-nav.navbar-left>li{
	width:235px;
	font-size:16px;
}
.navbar-inverse .navbar-nav.navbar-right>li{
	/*padding-top:20px;*/
}
.navbar-inverse .navbar-nav.navbar-right>li>a{
	font-size:16px;
	color:#eb590e;
	padding-top:40px;
	padding-bottom:40px;
}
.navbar-inverse .navbar-nav>li>a.menuBtn{
	background-color:#eb590e;
	border-radius:25px;
	padding-left:30px;
	padding-right:30px;
	font-size:16px;
	padding-top:15px;
	padding-bottom:15px;
	color:#fff;
}
.navbar-inverse .navbar-nav.navbar-right>li a:hover{
	/*color:#fff;*/
}
.navbar-inverse .navbar-nav>li>a.menuBtn:focus, .navbar-inverse .navbar-nav>li>a.menuBtn:hover{
	background-color:#fff;
	color:#eb590e;
}
.navbar-inverse .navbar-nav>.open>a, .navbar-inverse .navbar-nav>.open>a:focus, .navbar-inverse .navbar-nav>.open>a:hover{
	background-color:#fff;
	color:#eb590e;
}

.dropdown-menu{
	background-color:#fff;
	border:none;
	box-shadow:none;
	min-width:320px;
}
.navbar-inverse .navbar-nav>li>a{
	padding-left:20px;
	padding-right:20px;
}
.navbar-inverse .navbar-nav>li>a.active{
	background-color:#fff;
	color:#1c9287;
	font-weight:bold;
}
.navbar-inverse .navbar-nav>li>a:hover{
	/*background-color:#00b29f;
	color:#fff;*/
}
.dropdown-menu>li>a{
	padding:25px 30px;
	font-size:16px;
	color:#eb590e;
}
.navbar-inverse .navbar-nav>li h4{
	font-size:16px;
	color:#000;
	padding-left:25px;
	padding-right:25px;
	padding-top:20px;
}
.dropdown .menu-arrow{
	background:url(../images/nav_down.png) no-repeat 0% 0%;
	width:15px;
	height:10px;
	display:inline-block;
    margin-left:2px;
    vertical-align:middle;
}
.dropdown.open .menu-arrow{
	background:url(../images/nav_up.png) no-repeat 0% 0%;
}

/* Home page Styles */
.Section_0.home{
	background:#f2e2d2;
	padding-bottom:0px;
	text-align:center;
}
.Section_0.home img.responsive{
	max-width: 100%;
	width: 100%;
}
.Section_0.home .carousel-caption h1{
	text-transform:uppercase;
	margin-bottom:30px;
}
.home .col-sm-12 {
  margin-bottom: 20px;
  text-align:center;
}
.Section_1.home{
	background:#f2e2d2;
	padding-top:30px;
	padding-bottom:30px;
}
.Section_1.home h4{
	text-transform:uppercase;
}
.Section_1.home p{
	color:#866a5b;
}
.Section_2.home{
	background:#fffaf0;
	padding-top:30px;
	padding-bottom:30px;
}
.Section_3.home{
	background:#48403c;
}
.Section_3 .col-sm-6{
	padding-top:30px;
}
.Section_3.home h3{
	color:#a38567;
}
.Section_3.home p{
	font-size:14px;
	color:#8a8583;
}
.Section_3.home span.name{
	color:#f2e2d2;
}
.Section_3.home p.uppercase{
	text-transform:uppercase;
}
.spacebar {
    margin-left:10px;
    margin-right:10px;
}

a.btn, button.btn{
	background-color:#eb590e;
	border-radius:25px;
	padding-left:30px;
	padding-right:30px;
	font-size:18px;
	color:#eaf0ef;
}
a.btn:hover, button.btn:hover{
	background-color:#171819;
	color:#fff;	
}

.Section_3.home a.btn{
	background-color:none;
	background:none;
	border:2px solid #f2e2d2;
}
.Section_3.home a.btn:hover{
	background-color:#eb590e;
}
.Section_3.home .col-md-8{
	padding-top:50px;
}

.signUp{
	width:780px;
	margin:0 auto;
}
.signUp p{
	margin-bottom:5px;
}
.signUp .col-sm-12, 
.signUp .col-sm-6,
.signUp .col-sm-4{
	margin-bottom:10px;
}

.signUp input[type="text"],
.signUp input[type="email"],
.signUp input[type="tel"]{
	width:100%;
    height:45px;
    padding:5px;
	/*color:#868686;*/
	color:#866a5b;
    font-size:16px;
	border:none;
	background:#fffaf0;
	text-align:left;
	border-bottom:3px solid #f2e2d2;
}
label {
    position: relative;
	display:block;
	color:#866a5b;
	z-index:1;
}
label:hover span {
    /*display: none;*/
	z-index:-1;
}

input[type="text"]:focus, 
input[type="text"]:active, 
input[type="text"].active {
    z-index: 2;
}

label input[type="text"] {
    position: relative;
}
.title {
    color:#866a5b;
    position: absolute;
    left: 5px;
    top: 10px;
    z-index: 1;
}
.select-style select .title .symbol,
.symbol {
    color:#eb590e;
}
.select-style {
    padding:0;
    margin:0;
    width:100%;
    border-radius:3px;
	border:none;
    overflow:hidden;
    background-color:none;
    background: url("../images/arrow_down.png") no-repeat 90% 30%;
}

.select-style select {
    padding:9px 0px;
    width:130%;
    border:none;
    box-shadow:none;
    background-color:transparent;
    background-image:none;
	color:#866a5b;
    font-size:16px;
	text-align:left;
	border-bottom:3px solid #f2e2d2;
    -webkit-appearance: none;
       -moz-appearance: none;
            appearance: none;
}

.select-style select:focus {
    outline:none;
}

input, 
.select-style select,
.title{
	font-family: 'Montserrat', sans-serif;
	font-weight:400;
	color:#866a5b;
	font-size:16px;
}
.hiddenRadio{
	opacity:0;
	float:left;
}
.signUp a.btn, .signUp button.btn{
	width:100%;
}
.requiredText, .submitDiv{
	margin-top:20px;
}
.error{
	color:#F00;
	text-align:left;
	float:left;
	font-weight:normal;
}


.events{
	width:100%;
	float:left;
	margin-bottom:10px;
}
.events>div{
	float:left;
	margin-right:10px;
}
p.venue{
	font-size:16px;
	margin-bottom:0px;
	margin-top:0px;
	color:#866a5b;
}
p.location{
	font-size:14px;
	margin-bottom:0px;
	text-transform:uppercase;
	color:#8a8583;
}
.locationIcon{
	background:url(../images/locoation_icon.png) no-repeat 0% 0%;
	width:29px;
	height:35px;
}
.locdetails{
	width:90%;
}
.events-details{
	width:100%;
}
.events-details>div{
	width:100%;
	float:left;
}
p.small{
	font-size:14px;
	margin-bottom:0px;
}
p.small.semibold{
	font-weight:600;
}
.borderRight:before{
	content:'';
	width:2px;
	height:100%;
	background:#ddcfc2;
	position:absolute;
	top:0;
	right:50px;
}
.radioDiv{
	width:500px;
	margin:0 auto;
}
.radioDiv>div{
	float:left;
	border:2px solid #eb590e;
	display:inline-block;
	border-radius:25px;
    padding:8px 30px;
    font-size:18px;
	margin-right:15px;
	cursor:pointer;
}
.radioDiv>div.selected{
	background:#eb590e;
	color:#fff;
}

footer{
	background:#fffaf0;
}
.socialLinks, .footerLinks, .footerCopy{
	text-align:center;
}
.socialLinks{
	padding-top:30px;
	padding-bottom:20px;
	background:#fffaf0;
}
.footerLinks{
	margin-top:20px;
}
.footerCopy{
	padding-top:10px;
	background:#2a2828;
}
.socialLinks a{
	margin-right:10px;
}
.footeLinksDiv p, .footeLinksDiv p a{
	text-align:left;
	text-transform:uppercase;
}
.footeLinksDiv p{
	font-size:12px;
	color:#8a8583;
	margin-bottom:20px;
}
.footeLinksDiv p a{
	font-size:12px;
	margin-right:3px;
	margin-left:3px;
}
.footerCopy p{
	font-size:12px;
	color:#8a8583;
}
footer .organised{
	margin-left:0px;
}
footer .heldin{
	margin-left:10px;
}
footer .official{
	margin-left:70px;
}
.footeLinksDiv{
	width:1080px;
	margin:0 auto;
}
.footeLinksDiv>.inner{
	padding:20px 0;
	float:left;
}
.footeLinksDiv>.inner>div{
	float:left;
	margin-left:20px;
}
.footeLinksDiv>.inner>div.first>a{
	display:inline-block;
	margin-right:15px;
}
.footeLinksDiv>.inner>div.third,
.footeLinksDiv>.inner>div.fourth{
	margin-top:35px;

}
.modal{
	top:10%;	
}

