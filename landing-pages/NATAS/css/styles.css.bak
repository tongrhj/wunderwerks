.navbar-toggle .icon, #menu-btn .icon{
	background: url(../images/menu_icon.png);
	width:41px;
	height:33px;
	display:inline-block;
    margin-right:5px;
    vertical-align:middle;
}

.Section_1.home img,
.Section_0.partner img{
	max-width:100%;
}
.mobileBar{
	display:none;
	width:100%;
	float:left;
}
.mobileBar .logo{
	position:absolute;
	top:0px;
	left:10px;
	z-index:20;
}
.mobileBar .logo img{
	height:80px;
}
.carousel-caption h1{
	font-size:54px;
}

@media (min-width: 768px) {
	.partner .container{
		width:870px;
	}
	.navbar-inverse .navbar-nav>li>a.menuBtn{
		margin-top:25px;
	}
	.navbar-right{
		margin-right:0;
	}
	.navbar-inverse .navbar-nav.navbar-left>li{
		width:115px;
	}
}
@media (min-width: 992px) {
	.navbar-inverse .navbar-nav>li>a.menuBtn{
		margin-top:25px;
	}
	.navbar-inverse .navbar-nav.navbar-left>li{
		width:235px;
	}
	
}
@media (min-width: 1200px) {
	.navbar-right{
		margin-right:25px;
	}
	.navbar-inverse .navbar-nav>li>a {
		padding-left:25px;
		padding-right:25px;
	}
	.container{
		width:1080px;
	}
	
}

@media screen and (max-width: 768px){
	html{
		overflow-x:hidden;/**/
	}
	h1, .carousel-caption h1{
		font-size:38px;
		
	}
	h2{
		font-size:38px;
	}
	h3{
		font-size:32px;
	}
	.page{
		margin-top:0px;
	}
	.navbar-inverse{
		background:#eb590e;
	}
	#navbar{
		display:none;
		width:100%;
	}
	#menu-btn{
		display:block;
	}
	.navbar-left{
		display:none;
	}
	.navbar-collapse,
	.navbar-right{
		width:100%;
		margin:0;
		padding:0;
	}
	.mobileBar{
		display:block;
	}
	.navbar{
		padding-top:0;		
		margin-bottom:0;
		min-height:70px;
		height:auto;
	}
	.navbar-wrapper .navbar{
		padding-right:0;
		padding-left:0;
	}
	.navbar-right{
		position:fixed;
		left:0px;
		top:70px;
		z-index:10000000;
		width:100%;
		background:#fffaf0;
	}
	.navbar-inner .container{
		padding-right:0;
		padding-left:0;
	}		
	.navbar-nav>li>a{
		padding-top:25px;
		padding-bottom:25px;
		border-bottom:1px solid #fff;
	}
	.navbar-inverse .navbar-nav>li>a:focus, .navbar-inverse .navbar-nav>li>a:hover{
		background-color:#585858;
	}
	.navbar-inverse .navbar-nav>li>a.active{
		background-color:#585858;
		color:#fff;
	}	
	.navbar-inverse .navbar-collapse{
		margin-top:60px;
		border:none;
	}
	.navbar-inverse .navbar-nav.navbar-right>li>a{
		padding-top:25px;
		padding-bottom:25px;
	}
	.dropdown-menu>li>a{
		padding:25px 30px !important;
		color:#eb590e !important;
		background:#fff !important;
		border-bottom:1px solid #eb590e;
	}
	.dropdown-menu>li>a:hover{
		background-color:#585858 !important;
		color:#fff !important;
	}
	.navbar-inverse .navbar-nav>.open>a{
		/*background-color:#585858;*/
	}

	.Section_0.home{
		margin-top:70px;
	}
	.Section_0.home .carousel-caption{
		position:relative;
		left:0;		
		top:0;
		width:100%;
		background:#fff;
	}
	.Section_2.home{
		height:auto;;
		padding-top:20px;
	}
	.Section_3.home{
		height:auto;
		-webkit-background-size: cover;
			background-size: cover;
	}
	.Section_3.home a.btn{
		padding-left:10px;
		padding-right:10px;
		font-size:16px;
	}

	.signUp{
		width:100%;
	}

	.borderRight:before{
		display:none;
	}
	footer{
		height:auto;
	}
	.footerCopy{
		padding-bottom:10px;
	}
	.footeLinksDiv{
		width:320px;
		
	}
	.footeLinksDiv>.inner>div{
		margin-left:15px;
		margin-bottom:30px;
		text-align:center;
		width:100%;
	}
	.footeLinksDiv>.inner{
	}
	.footeLinksDiv p{
		margin-bottom:10px;
		text-align:center;
	}
	footer .official{
		margin-left:0;
	}
	footer .organised{
		margin-left:-40px;
	}
	.footeLinksDiv>.inner>div{
		margin-left:0;
	}
	.footeLinksDiv>.inner>div.second,
	.footeLinksDiv>.inner>div.fourth{
		margin-left:0;
	}	
	.footeLinksDiv>.inner>div.fourth .marginRight{
		margin-right:30px;
	}
	.footeLinksDiv>.inner>div.second .marginRight{
		margin-right:18px;
	}
	.footeLinksDiv>.inner>div.third,
	.footeLinksDiv>.inner>div.fourth{
		margin-top:0px;
	}

	/*.Section_0 img, */
	.Section_1 img,
	.Section_2 img, 
	.Section_4 img,
	.Section_6 img,
	.Section_8 img
	{
		width:100%;
	}
	.radioDiv{
		width:100%;
	}
	.radioDiv>div{
		margin-bottom:10px;
	}
	a.btn, button.btn{
		font-size:16px;
	}
	.Section_3.home{
		background:none;
		
	}
	.Section_3.home .col-md-8{
		background:#48403c;
		padding-top:20px;
		padding-bottom:30px;
	}
	.locdetails{
		width:80%;
	}
	.events-details>div{
		width:50%;
	}
	.events-details,.events{
		clear:both;
		width:100%;
		float:left;		
	}

	.events-details>div{
		position:relative;
		margin-top:20px;
		margin-bottom:20px;
		padding-left:15px;
		text-align:left;
	}
	.events-details>div:first-child{
		border-right:2px solid #ddcfc2;
		padding-right:15px;
		padding-left:0px;
		text-align:right;
	}
	

}