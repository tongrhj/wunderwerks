	font-size:20px;
    color:#fff;
	border:none;
}
.navbar-inverse .navbar-toggle:hover{
	background:none;
}
.navbar-inverse .navbar-toggle:focus,.navbar-inverse .navbar-toggle:hover{background:none;}
.navbar-wrapper {
	position: absolute;
	top: 0;
	right: 0;
	left: 0;
	z-index: 20;
}
#menu-btn{
	/*font-size:20px;
    color:#fff;
    border:none;
	position:relative;
	z-index:10000;
